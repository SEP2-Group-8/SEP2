<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.*?>
<AnchorPane prefHeight="800.0" prefWidth="1069.0" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="dk.via.sep.client.view.userList.UserListViewController">
    <children>
        <AnchorPane prefHeight="880.0" prefWidth="200.0" style="-fx-background-color: #128c33;">
            <children>
                <VBox layoutY="159.0" prefHeight="308.0" prefWidth="200.0">
                    <children>
                        <Button mnemonicParsing="false" onAction="#onHomeButton" prefHeight="77.0" prefWidth="201.0"
                                style="-fx-background-color: transparent;" text="Home">
                            <font>
                                <Font size="25.0"/>
                            </font>
                        </Button>
                        <Button layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#onProfileButton"
                                prefHeight="77.0" prefWidth="201.0" style="-fx-background-color: transparent;"
                                text="Profile">
                            <font>
                                <Font size="25.0"/>
                            </font>
                        </Button>
                        <Button layoutX="10.0" layoutY="87.0" mnemonicParsing="false" onAction="#onEventsButton"
                                prefHeight="77.0" prefWidth="201.0" style="-fx-background-color: transparent;"
                                text="Events">
                            <font>
                                <Font size="25.0"/>
                            </font>
                        </Button>
                        <Button layoutX="10.0" layoutY="164.0" mnemonicParsing="false" onAction="#onChatButton"
                                prefHeight="77.0" prefWidth="201.0" style="-fx-background-color: transparent;"
                                text="Chat">
                            <font>
                                <Font size="25.0"/>
                            </font>
                        </Button>
                        <Button layoutX="10.0" layoutY="241.0" mnemonicParsing="false" onAction="#onUsersButton"
                                prefHeight="77.0" prefWidth="200.0" style="-fx-background-color: white;" text="Users">
                            <font>
                                <Font size="25.0"/>
                            </font>
                        </Button>
                    </children>
                </VBox>
            </children>
        </AnchorPane>
        <TableView fx:id="userTable" layoutX="219.0" layoutY="340.0" prefHeight="522.0" prefWidth="830.0">
            <columns>
                <TableColumn fx:id="nameColumn" prefWidth="387.0" text="Name"/>
                <TableColumn fx:id="emailColumn" minWidth="0.0" prefWidth="442.0" text="E-mail"/>
            </columns>
        </TableView>
        <ButtonBar layoutX="466.0" layoutY="289.0" prefHeight="40.0" prefWidth="337.0">
            <buttons>
                <Button mnemonicParsing="false" text="Add Admin"/>
                <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="356.0" text="Modify User"/>
                <Button mnemonicParsing="false" prefHeight="25.0" text="Remove User"/>
            </buttons>
        </ButtonBar>
        <Text layoutX="454.0" layoutY="66.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Via Football Club">
            <font>
                <Font size="48.0"/>
            </font>
        </Text>
    </children>
</AnchorPane>
